{
  "name": "bikehopper-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@formatjs/intl-listformat": "^7.1.7",
    "@headlessui/react": "^1.7.14",
    "@nivo/core": "^0.85.1",
    "@nivo/line": "^0.85.1",
    "@placemarkio/geo-viewport": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "^2.0.4",
    "@radix-ui/react-radio-group": "^1.1.2",
    "@radix-ui/react-select": "^1.2.1",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@turf/bezier-spline": "^6.5.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/buffer": "^6.5.0",
    "@turf/convex": "^6.5.0",
    "@turf/distance": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "@turf/length": "^6.5.0",
    "@turf/line-slice-along": "^6.5.0",
    "@turf/meta": "^6.5.0",
    "@turf/transform-rotate": "^6.5.0",
    "bowser": "^2.11.0",
    "browserslist": "^4.18.1",
    "classnames": "^2.3.1",
    "color": "^4.2.1",
    "eslint": "^8.3.0",
    "history": "^5.3.0",
    "iconoir": "^7.6.0",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "luxon": "^3.2.1",
    "maplibre-gl": "^1.15.2",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.2.5",
    "react-map-gl": "^7.0.6",
    "react-redux": "^8.0.5",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.2.1",
    "react-spinners": "^0.13.7",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "resolve": "^1.20.0",
    "semver": "^7.3.5",
    "web-vitals": "^3"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "extract": "formatjs extract 'src/**/*.{js,jsx}' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile",
    "prepare": "husky"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.12",
    "@tailwindcss/typography": "^0.5.9",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "babel-plugin-formatjs": "^10.3.35",
    "csv": "^6.2.5",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-formatjs": "^4.5.0",
    "husky": "^9.0.11",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-resolve": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^2.4.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}

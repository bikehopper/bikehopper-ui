{
  "compilerOptions": {
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "target": "es2020",
    "module": "ESNext",
    "jsx": "preserve",
    "moduleResolution": "Bundler",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vite/client", "vite-plugin-svgr/client"]
  },
  "include": ["./src/**/*"]
}
